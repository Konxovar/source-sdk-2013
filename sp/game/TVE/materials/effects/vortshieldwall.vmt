"unlitgeneric"
{
	"$basetexture" "Effects/vortshieldwall"

	"$translucent" "1"
	"$envmap" "env_cubemap"
	"$envmaptint" "[1 1 1]"

	"Proxies"
	{
		"TextureScroll"
		{
			"texturescrollvar" "$basetexturetransform"
			"texturescrollrate" .5
			"texturescrollangle" -120
		}
"AnimatedTexture"
{
"animatedTextureVar" "$basetexture"
"animatedTextureFrameNumVar" "$frame"
"animatedTextureFrameRate" "12"
}
	}
}